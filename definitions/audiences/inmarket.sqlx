config {
    type: 'table',
    bigquery: {
        partitionBy: 'dt',
        clusterBy: ['sha256_lc_hem', 'up_id', 'dt','category']
    }
}
-- This query aggregates inmarket intents by user and date
select 
  dt,
  sha256_lc_hem, 
  array_agg(struct(parent,category,subcategory,topic) limit 300) as intents 
from ${ref("onpointdata","staging_inmarket")} 
group by dt,sha256_lc_hem